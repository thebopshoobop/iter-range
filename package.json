  "devDependencies": {
    "codecov": "^3.0.0",
    "eslint": "^4.16.0",
    "eslint-plugin-jasmine": "^2.9.1",
    "git-hooks": "^1.1.9",
    "istanbul": "^0.4.5",
    "jasmine": "^2.9.0",
    "jsdoc-to-markdown": "^4.0.1",
    "nyc": "^11.4.1"
